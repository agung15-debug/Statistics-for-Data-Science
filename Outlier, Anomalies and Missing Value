   ],
   "source": [
    "# Finding the IQR which is the difference between third and first quartile\n",
    "\n",
    "iqr = q3 - q1\n",
    "\n",
    "# Print Value IQR\n",
    "\n",
    "print(iqr)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
